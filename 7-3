public class AutoBoxingUnboxing {

    public static void main(String[] args) {
        int num = 10;
        Integer obj = num; // Autoboxing

        int num2 = obj; // Unboxing
        System.out.println("Autoboxing: int to Integer - " + obj);
        System.out.println("Unboxing: Integer to int - " + num2);

        char ch = 'A';
        Character objChar = ch; // Autoboxing
        char ch2 = objChar; // Unboxing
        System.out.println("Autoboxing: char to Character - " + objChar);
        System.out.println("Unboxing: Character to char - " + ch2);

        double d = 5.5;
        Double objDouble = d; // Autoboxing
        double d2 = objDouble; // Unboxing
        System.out.println("Autoboxing: double to Double - " + objDouble);
        System.out.println("Unboxing: Double to double - " + d2);
    }
}
